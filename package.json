{
  "name": "@bryance/wikit",
  "version": "0.1.0",
  "description": "CLI and TUI for managing Wiki.js instances - sync pages, manage users, and more",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "wikit": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./commands": {
      "import": "./dist/commands/index.js",
      "types": "./dist/commands/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    }
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "tsc": "tsc --noEmit",
    "lint": "eslint src",
    "check": "tsc --noEmit && eslint src",
    "build": "tsc && bunx tsc-alias -p tsconfig.json --resolve-full-paths",
    "start": "node dist/index.js",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "wikijs",
    "wiki.js",
    "wiki",
    "cli",
    "tui",
    "api",
    "management",
    "admin",
    "sync",
    "migration"
  ],
  "author": "Dale Hendersen",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/bryance/wikit"
  },
  "bugs": {
    "url": "https://github.com/bryance/wikit/issues"
  },
  "homepage": "https://github.com/bryance/wikit#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^14.0.1",
    "dotenv": "^16.4.5",
    "font-list": "^2.0.1",
    "ink": "^6.3.1",
    "pino": "^9.12.0",
    "react": "^19.1.1",
    "react-devtools-core": "^6.1.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/commander": "^2.12.5",
    "@types/react": "^19.1.13",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.44.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
